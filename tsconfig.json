{
    "compilerOptions": {
        "baseUrl": ".",
        "strict": true,
        "target": "ES6",
        "module": "ES6",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "paths": {
            "@core/*": [
                "core/*"
            ],
            "@constants/*": [
                "core/constants/*"
            ],
            "@fixtures/*": [
                "core/fixtures/*"
            ],
            "@pageObjects/*": [
                "pages/objects/*"
            ]
        }
    }
}